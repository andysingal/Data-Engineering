{% extends 'base.jinja2' %}

{% block content %}
<h1>Create post</h1>
<form action="/post/create" method="POST">
  <div>
     <label for="title">Title</label>
      <input type="text" name="title" />
  </div>
  <div>
      <label for="content">Content</label>
      <textarea name="content"></textarea>
  </div>
  <div>
      <input type="submit" />
  </div>
</form>
{% endblock %}